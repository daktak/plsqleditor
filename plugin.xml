   <extension
         point="org.eclipse.core.expressions.propertyTesters">
	  <propertyTester
	    class="plsqleditor.views.schema.DatabaseEntityTypeTester"
	    id="plsqleditor.views.schema.propertyTester.DatabaseEntityTypeTester"
