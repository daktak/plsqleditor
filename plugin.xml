               label="%action.label.8"
               menubarPath="plSqlMenu/plsqlGroup"
               style="push"
               toolbarPath="plsqlGroup"
               tooltip="%action.tooltip.6"/>
         <action
               class="plsqleditor.actions.WorkBenchWindowAction"
               icon="icons/target.gif"
