   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="plsql.editor"
            name="%context.name"
            description="%context.description"
            parentId="org.eclipse.ui.textEditorScope"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="plsql.editor"
            name="%category.name.0"/>
      <view
            category="plsql.editor"
            class="plsqleditor.views.ScratchPadView"
            icon="icons/pencil.gif"
            id="plsqleditor.views.ScratchPadView"
            name="%view.name"/>
      <view
            category="plsql.editor"
            class="plsqleditor.views.schema.SchemaBrowserView"
            icon="icons/browse.gif"
            id="plsqleditor.views.SchemaBrowserView"
            name="%view.name.0"/>
      <view
            category="plsql.editor"
            class="plsqleditor.views.DbmsOutputView"
            icon="icons/book.gif"
            id="plsqleditor.views.DbmsOutputView"
            name="%view.name.1"/>
   </extension>
   <extension
      point="org.eclipse.compare.contentMergeViewers">
      <viewer
            extensions="pkbx"
            class="plsqleditor.compare.PlsqlContentViewerCreator"
            id="plsqleditor.compare.PlsqlContentViewerCreator">
      </viewer>
   </extension>
   
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="plsqleditor.perspective">
         <actionSet id="plsqleditor.actionSet"/>
         <view
               id="plsqleditor.views.ScratchPadView"
               ratio="0.5"
               relationship="stack"
               relative="org.eclipse.ui.views.TaskList"/>
         <view
               id="plsqleditor.views.DbmsOutputView"
               ratio="0.5"
               relationship="stack"
               relative="org.eclipse.ui.views.TaskList"/>
         <view
               id="plsqleditor.views.SchemaBrowserView"
               ratio="0.5"
               relationship="stack"
               relative="org.eclipse.ui.views.ResourceNavigator"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="plsqleditor.Perspective"
            fixed="false"
            icon="icons/at.png"
            id="plsqleditor.perspective"
            name="%perspective.name"/> 
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="plsqleditor.loadToDatabase"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Alt+L"/>
      <key
            commandId="plsql.file.lookup"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F3"/>
      <scheme
            id="plsqleditor.keys"
            name="%scheme.name"/>
      <key
            commandId="plsqleditor.generateHeader"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Alt+H"/>
      <key
            commandId="plsqleditor.refreshErrorStatus"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Alt+R"/>
      <key
            commandId="plsql.file.executeSql"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Alt+E"/>
      <key
            commandId="plsqleditor.lowerCase"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Alt+O"/>
      <key
            commandId="plsqleditor.upperCase"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Alt+U"/>
      <key
            commandId="plsqleditor.commentblock"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+/"/>
      <key
            commandId="plsqleditor.uncommentblock"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Shift+/"/>
      <key
            commandId="plsqleditor.generatePlDoc"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Alt+D"/>
      <key
            commandId="plsqleditor.commands.jumpToBracket"
            contextId="plsql.editor"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+Shift+P" />            
      <!--key
            sequence="M1+M2+P"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            commandId="plsqleditor.commands.jumpToBracket"
            contextId="plsql.editor" /-->            
   </extension>
   <extension
         point="org.eclipse.ui.editors.templates">
      <contextType name="%plsql.contextType.name" class="plsqleditor.template.PlSqlContextType" id="plsqleditor.template.plsql" /> 
      <contextType name="%pldoc.contextType.name" class="plsqleditor.template.PlDocContextType" id="plsqleditor.template.pldoc" /> 
      <include file="templates/plsql.xml" translations="templates/plsql.properties" />
      <resolver
            class="plsqleditor.template.PlSqlExceptionResolver"
            contextTypeId="plsqleditor.template.plsql"
            description="%plsql.resolvers.plsqlexception.description"
            icon="icons/worker.png"
            name="%plsql.resolvers.plsqlexception"
            type="plsqlexception"/>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="plsqleditor.properties.ProjectDBPropertyPage"
            id="plsqleditor.properties.databaseConnectivity"
            name="%page.name.7"
            nameFilter="*"
            objectClass="org.eclipse.core.resources.IProject"/>
      <page
            class="plsqleditor.properties.SchemaMappingPreferencePage"
            id="plsqleditor.properties.schemaMapping"
            name="%page.name.8"
            nameFilter="*"
            objectClass="org.eclipse.core.resources.IProject"/>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
         <menuContribution
            locationURI="popup:org.eclipse.ui.views.ResourceNavigator">
         <command
               commandId="plsqleditor.generateHeaderAndLoadBoth"
               label="%command.label"
               style="push">
               <visibleWhen checkEnabled="false">
				    <iterate ifEmpty="false">
				       <adapt type="org.eclipse.core.resources.IFile">
				          <test property="org.eclipse.core.resources.name" 
				                value="*.pkb"/>
				       </adapt>
				    </iterate>
				</visibleWhen>
         </command>
         </menuContribution>
         <menuContribution
               locationURI="popup:org.eclipse.ui.views.ResourceNavigator">
            <command
                  commandId="boomsticks.plsqleditor.loadSchema"
                  label="%command.label.0"
                  style="push">
            </command> 
         </menuContribution>
         <menuContribution
               locationURI="popup:org.eclipse.ui.views.ResourceNavigator">
            <command
                  commandId="boomsticks.plsqleditor.compareToDb"
                  label="%command.label.1"
                  style="push">
				<!--visibleWhen checkEnabled="false">
				      <reference
 				        definitionId="TBD">
				      </reference>
 				</visibleWhen-->
             </command>
         </menuContribution>         
         <menuContribution
               locationURI="popup:plsqleditor.views.SchemaBrowserView">
            <command
                  commandId="boomsticks.plsqleditor.loadSourceFromDb"
                  label="%command.label.2"
                  style="push">
                  <visibleWhen checkEnabled="false">
                      <reference
 				        definitionId="isSourceLoadableFromTreeObject">
				      </reference>
				    <!--iterate ifEmpty="false"> ... this works
				       <adapt type="plsqleditor.views.schema.IPackageData">
				       </adapt>
				    </iterate-->
				</visibleWhen>
            </command> 
         </menuContribution>
         <menuContribution
               locationURI="popup:plsqleditor.views.SchemaBrowserView">
            <command
                  commandId="boomsticks.plsqleditor.showGrants"
                  label="%command.label.3"
                  style="push">
				<visibleWhen checkEnabled="false">
				      <reference
 				        definitionId="isGrantLoadableFromTreeObject">
				      </reference>
 				</visibleWhen>
             </command>
         </menuContribution>
         <menuContribution
               locationURI="popup:plsqleditor.views.SchemaBrowserView">
            <command
                  commandId="boomsticks.plsqleditor.showDetails"
                  label="%command.label.4"
                  style="push">
				<visibleWhen checkEnabled="false">
				      <reference
 				        definitionId="isDetailsAvailableFromTreeObject">
				      </reference>
 				</visibleWhen>
             </command>
         </menuContribution>
   </extension>
   <extension
         id="tz"
         name="tz"
         point="org.eclipse.ui.handlers">
      <handler
            class="plsqleditor.actions.GenerateHeaderAndLoadBothToDatabaseAction"
            commandId="plsqleditor.generateHeaderAndLoadBoth">
      </handler>
      <handler
            class="plsqleditor.actions.LoadSchemaToDb"
            commandId="boomsticks.plsqleditor.loadSchema">
      </handler>
      <handler
            class="plsqleditor.actions.schemabrowser.LoadSourceFromDb"
            commandId="boomsticks.plsqleditor.loadSourceFromDb">
            <activeWhen>
	        <reference
	          definitionId="isSourceLoadableFromTreeObject">
	        </reference>
	      </activeWhen>
      </handler>
      <handler
            class="plsqleditor.actions.schemabrowser.ShowGrants"
            commandId="boomsticks.plsqleditor.showGrants">
          <activeWhen>
	        <reference
	          definitionId="isGrantLoadableFromTreeObject">
	        </reference>
	      </activeWhen>
      </handler>
      <handler
            class="plsqleditor.actions.schemabrowser.ShowDetails"
            commandId="boomsticks.plsqleditor.showDetails">
          <activeWhen>
	        <reference
	          definitionId="isDetailsAvailableFromTreeObject">
	        </reference>
	      </activeWhen>
      </handler>
      <handler
            class="plsqleditor.actions.CompareToDb"
            commandId="boomsticks.plsqleditor.compareToDb">
          <!--activeWhen>
	        <reference
	          definitionId="someDefinitionTBD">
	        </reference>
	      </activeWhen-->
      </handler>
   </extension>
